<doctrine-mapping xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\DataAccessBundle\Entity\CoreTableConfiguration\CmsTblFieldTab"
            table="cms_tbl_field_tab">
        <id name="id" type="string" length="36" column="id" unique="true"/>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>
        <many-to-one field="cmsTblConf"
                     target-entity="\ChameleonSystem\DataAccessBundle\Entity\CoreTableConfiguration\CmsTblConf"
                     inversed-by="cmsTblFieldTabCollection">
            <options>
                <option name="comment">Belongs to table</option>
            </options>
            <join-column name="cms_tbl_conf_id" referenced-column-name="id"/>
        </many-to-one>
        <field name="name" type="string" column="name" length="255" nullable="false">
            <options>
                <option name="comment">Name</option>
                <option name="default"></option>
            </options>
        </field>
        <field name="position" type="integer" column="position" nullable="false">
            <options>
                <option name="comment">Position</option>
                <option name="default"></option>
            </options>
        </field>
        <field name="systemname" type="string" column="systemname" length="255" nullable="false">
            <options>
                <option name="comment">System name</option>
                <option name="default"></option>
            </options>
        </field>
        <field name="description" type="text" column="description" nullable="false">
            <options>
                <option name="comment">Description</option>
            </options>
        </field>
        <lifecycle-callbacks>
        </lifecycle-callbacks>

    </entity>

</doctrine-mapping>