<doctrine-mapping xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\DataAccessBundle\Entity\CoreModule\ModuleListCat" table="module_list_cat">
        <id name="id" type="string" length="36" column="id" unique="true"/>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>
        <many-to-one field="cmsTplModuleInstance"
                     target-entity="\ChameleonSystem\DataAccessBundle\Entity\CorePagedef\CmsTplModuleInstance">
            <join-column name="cms_tpl_module_instance_id" referenced-column-name="id"/>
            <options>
                <option name="comment">Module instance</option>
            </options>
        </many-to-one>
        <field name="name" type="string" column="name" length="255" nullable="false">
            <options>
                <option name="comment">Title</option>
                <option name="default"></option>
            </options>
        </field>
        <field name="sortOrder" type="integer" column="sort_order" nullable="false">
            <options>
                <option name="comment">Sorting order</option>
                <option name="default"></option>
            </options>
        </field>
        <lifecycle-callbacks>
        </lifecycle-callbacks>

    </entity>

</doctrine-mapping>