                <target><![CDATA[Der Newsletter wurde für den Versand vorbereitet.<br/>
%count% Abonnenten wurden in die Versandliste übertragen.]]></target>
            </trans-unit>
            <trans-unit id="chameleon_system_newsletter.action.clear_queue">
                <source>chameleon_system_newsletter.action.clear_queue</source>
                <target>Warteschlangen-Einträge löschen</target>
            </trans-unit>
            <trans-unit id="chameleon_system_newsletter_group.action.export_subscriber">
                <source>chameleon_system_newsletter_group.action.export_subscriber</source>
