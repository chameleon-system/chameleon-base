<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopWishlistMailHistory" table="pkg_shop_wishlist_mail_history">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="pkgShopWishlist" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopWishlist" inversed-by="pkgShopWishlistMailHistoryCollection" >
    <join-column name="pkg_shop_wishlist_id" referenced-column-name="id" />
</many-to-one>
            <field name="toName" type="string" column="to_name" length="255" nullable="false"  />
            <field name="toEmail" type="string" column="to_email" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>