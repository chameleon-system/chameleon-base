            <field name="headline" column="headline" type="string"  nullable="false"  />
            <field name="showAllLinkName" column="show_all_link_name" type="string"  nullable="false"  />
            <field name="pkgShopRatingServiceMlt" column="pkg_shop_rating_service_mlt" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>