<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ModuleFeedback" table="module_feedback">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="cmsTplModuleInstanceId" type="string" field="cms_tpl_module_instance_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsTplModuleInstance" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTplModuleInstance">
    <join-column name="cms_tpl_module_instance_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="text"  column="text"  nullable="false"  />
            <field name="doneText"  column="done_text"  nullable="false"  />
            <field name="toEmail"  column="to_email"  nullable="false"  />
            <field name="bccEmail"  column="bcc_email"  nullable="false"  />
            <field name="fromEmail"  column="from_email"  nullable="false"  />
            <field name="defaultSubject"  column="default_subject"  nullable="false"  />
            <field name="defaultBody"  column="default_body"  nullable="false"  />
    </entity>

</doctrine-mapping>