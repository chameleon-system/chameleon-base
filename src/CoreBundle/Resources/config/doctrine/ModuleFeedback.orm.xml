<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\ModuleFeedback" table="module_feedback">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="cmsTplModuleInstanceId" column="cms_tpl_module_instance_id" type="string"  nullable="false"  />
            <field name="name" column="name" type="string" length="80" nullable="false"  />
            <field name="text" column="text" type="string"  nullable="false"  />
            <field name="doneText" column="done_text" type="string"  nullable="false"  />
            <field name="toEmail" column="to_email" type="string" length="255" nullable="false"  />
            <field name="bccEmail" column="bcc_email" type="string"  nullable="false"  />
            <field name="fromEmail" column="from_email" type="string"  nullable="false"  />
            <field name="defaultSubject" column="default_subject" type="string" length="255" nullable="false"  />
            <field name="defaultBody" column="default_body" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>