<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\ShopArticleReview" table="shop_article_review">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="shopArticleId" column="shop_article_id" type="string"  nullable="false"  />
            <field name="dataExtranetUserId" column="data_extranet_user_id" type="string"  nullable="false"  />
            <field name="publish" column="publish" type="string"  nullable="false"  />
            <field name="authorName" column="author_name" type="string"  nullable="false"  />
            <field name="title" column="title" type="string"  nullable="false"  />
            <field name="authorEmail" column="author_email" type="string"  nullable="false"  />
            <field name="sendCommentNotification" column="send_comment_notification" type="string"  nullable="false"  />
            <field name="rating" column="rating" type="string"  nullable="false"  />
            <field name="helpfulCount" column="helpful_count" type="string"  nullable="false"  />
            <field name="notHelpfulCount" column="not_helpful_count" type="string"  nullable="false"  />
            <field name="actionId" column="action_id" type="string"  nullable="false"  />
            <field name="comment" column="comment" type="string"  nullable="false"  />
            <field name="datecreated" column="datecreated" type="string"  nullable="false"  />
            <field name="userIp" column="user_ip" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>