<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopArticleReview" table="shop_article_review">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopArticle" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopArticle" inversed-by="shopArticleReviewCollection" >
    <join-column name="shop_article_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="dataExtranetUser" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUser" inversed-by="shopArticleReviewCollection" >
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="authorName" type="string" column="author_name" length="255" nullable="false"  />
            <field name="title" type="string" column="title" length="255" nullable="false"  />
            <field name="authorEmail" type="string" column="author_email" length="255" nullable="false"  />
            <field name="rating" type="string" column="rating" length="255" nullable="false"  />
            <field name="helpfulCount" type="string" column="helpful_count" length="255" nullable="false"  />
            <field name="notHelpfulCount" type="string" column="not_helpful_count" length="255" nullable="false"  />
            <field name="actionId" type="string" column="action_id" length="255" nullable="false"  />
            <field name="userIp" type="string" column="user_ip" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>