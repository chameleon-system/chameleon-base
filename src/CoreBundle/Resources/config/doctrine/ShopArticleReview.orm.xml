<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopArticleReview" table="shop_article_review">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopArticleId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticle" inversed-by="shopArticleReview">
    <join-column name="shop_article_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="dataExtranetUserId" target-entity="ChameleonSystem\CoreBundle\Entity\DataExtranetUser" inversed-by="shopArticleReview">
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="publish"  column="publish"  nullable="false"  />
            <field name="authorName"  column="author_name"  nullable="false"  />
            <field name="title"  column="title"  nullable="false"  />
            <field name="authorEmail"  column="author_email"  nullable="false"  />
            <field name="sendCommentNotification"  column="send_comment_notification"  nullable="false"  />
            <field name="rating"  column="rating"  nullable="false"  />
            <field name="helpfulCount"  column="helpful_count"  nullable="false"  />
            <field name="notHelpfulCount"  column="not_helpful_count"  nullable="false"  />
            <field name="actionId"  column="action_id"  nullable="false"  />
            <field name="comment"  column="comment"  nullable="false"  />
            <field name="datecreated"  column="datecreated"  nullable="false"  />
            <field name="userIp"  column="user_ip"  nullable="false"  />
    </entity>

</doctrine-mapping>