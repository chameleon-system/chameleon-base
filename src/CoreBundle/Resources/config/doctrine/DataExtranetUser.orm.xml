<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\DataExtranetUser" table="data_extranet_user">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shop" target-entity="\ChameleonSystem\CoreBundle\Entity\Shop" inversed-by="dataExtranetUserCollection" >
    <join-column name="shop_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="cmsPortal" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsPortal">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="customerNumber" type="string" column="customer_number" length="255" nullable="false"  />
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="password" type="string" column="password" length="255" nullable="false"  />
            <field name="passwordChangeKey" type="string" column="password_change_key" length="255" nullable="false"  />
            <many-to-one field="dataExtranetSalutation" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetSalutation">
    <join-column name="data_extranet_salutation_id" referenced-column-name="id" />
</many-to-one>
            <field name="firstname" type="string" column="firstname" length="255" nullable="false"  />
            <field name="lastname" type="string" column="lastname" length="255" nullable="false"  />
            <field name="company" type="string" column="company" length="255" nullable="false"  />
            <field name="street" type="string" column="street" length="255" nullable="false"  />
            <field name="streetnr" type="string" column="streetnr" length="255" nullable="false"  />
            <field name="postalcode" type="string" column="postalcode" length="255" nullable="false"  />
            <field name="city" type="string" column="city" length="255" nullable="false"  />
            <many-to-one field="dataCountry" target-entity="\ChameleonSystem\CoreBundle\Entity\DataCountry">
    <join-column name="data_country_id" referenced-column-name="id" />
</many-to-one>
            <field name="vatId" type="string" column="vat_id" length="255" nullable="false"  />
            <field name="telefon" type="string" column="telefon" length="255" nullable="false"  />
            <field name="mobile" type="string" column="mobile" length="255" nullable="false"  />
            <field name="addressAdditionalInfo" type="string" column="address_additional_info" length="255" nullable="false"  />
            <field name="aliasName" type="string" column="alias_name" length="255" nullable="false"  />
            <field name="email" type="string" column="email" length="255" nullable="false"  />
            <field name="fax" type="string" column="fax" length="255" nullable="false"  />
            <one-to-many field="dataExtranetUserAddressCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUserAddress" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="sessionKey" type="string" column="session_key" length="32" nullable="false"  />
            <field name="loginTimestamp" type="string" column="login_timestamp" length="255" nullable="false"  />
            <field name="loginSalt" type="string" column="login_salt" length="255" nullable="false"  />
            <one-to-many field="dataExtranetUserLoginHistoryCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUserLoginHistory" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <many-to-one field="defaultBillingAddress" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUserAddress">
    <join-column name="default_billing_address_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="defaultShippingAddress" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUserAddress">
    <join-column name="default_shipping_address_id" referenced-column-name="id" />
</many-to-one>
            <field name="tmpconfirmkey" type="string" column="tmpconfirmkey" length="32" nullable="false"  />
            <one-to-many field="shopUserPurchasedVoucherCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopUserPurchasedVoucher" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopUserNoticeListCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopUserNoticeList" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopOrderCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrder" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="dataExtranetUserShopArticleHistoryCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUserShopArticleHistory" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopSearchLogCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSearchLog" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopSuggestArticleLogCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSuggestArticleLog" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopArticleReviewCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopArticleReview" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="pkgShopWishlistCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopWishlist" mapped-by="dataExtranetUser" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <many-to-one field="pkgShopCurrency" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopCurrency">
    <join-column name="pkg_shop_currency_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>