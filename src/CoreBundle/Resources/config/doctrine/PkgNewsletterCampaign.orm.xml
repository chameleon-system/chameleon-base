<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\PkgNewsletterCampaign" table="pkg_newsletter_campaign">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="utmSource" column="utm_source" type="string"  nullable="false"  />
            <field name="utmMedium" column="utm_medium" type="string"  nullable="false"  />
            <field name="utmContent" column="utm_content" type="string"  nullable="false"  />
            <field name="utmCampaign" column="utm_campaign" type="string"  nullable="false"  />
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="cmsTreeNodeId" column="cms_tree_node_id" type="string"  nullable="false"  />
            <field name="active" column="active" type="string"  nullable="false"  />
            <field name="subject" column="subject" type="string"  nullable="false"  />
            <field name="cmsPortalId" column="cms_portal_id" type="string"  nullable="false"  />
            <field name="pkgNewsletterQueue" column="pkg_newsletter_queue" type="string"  nullable="false"  />
            <field name="contentPlain" column="content_plain" type="string"  nullable="false"  />
            <field name="queueDate" column="queue_date" type="string"  nullable="false"  />
            <field name="sendStatistics" column="send_statistics" type="string"  nullable="false"  />
            <field name="sendStartDate" column="send_start_date" type="string"  nullable="false"  />
            <field name="sendEndDate" column="send_end_date" type="string"  nullable="false"  />
            <field name="generateUserDependingNewsletter" column="generate_user_depending_newsletter" type="string"  nullable="false"  />
            <field name="pkgNewsletterGroupMlt" column="pkg_newsletter_group_mlt" type="string"  nullable="false"  />
            <field name="googleAnalyticsActive" column="google_analytics_active" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>