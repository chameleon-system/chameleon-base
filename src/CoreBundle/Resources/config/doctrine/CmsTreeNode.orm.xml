<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTreeNode" table="cms_tree_node">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="active"  column="active"  nullable="false"  />
            <field name="startDate"  column="start_date"  nullable="false"  />
            <field name="endDate"  column="end_date"  nullable="false"  />
            <field name="tbl"  column="tbl"  nullable="false"  />
            <field name="contidId" type="string" field="contid_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="contid" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTplPage">
    <join-column name="contid_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="cmsTreeId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsTree" inversed-by="cmsTreeNode">
    <join-column name="cms_tree_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>