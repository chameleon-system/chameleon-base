<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTreeNode" table="cms_tree_node">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="tbl" type="string" column="tbl" length="255" nullable="false"  />
            <many-to-one field="con" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTplPage">
    <join-column name="contid" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="cmsTree" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTree" inversed-by="cmsTreeNodeCollection" >
    <join-column name="cms_tree_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>