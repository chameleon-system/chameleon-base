<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopOrderItem" table="shop_order_item">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="nameVariantInfo" type="string" column="name_variant_info" length="255" nullable="false"  />
            <many-to-one field="shopOrder" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrder" inversed-by="shopOrderItemCollection" >
    <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <field name="basketItemKey" type="string" column="basket_item_key" length="32" nullable="false"  />
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="articlenumber" type="string" column="articlenumber" length="255" nullable="false"  />
            <field name="shopManufacturerName" type="string" column="shop_manufacturer_name" length="255" nullable="false"  />
            <field name="stock" type="string" column="stock" length="255" nullable="false"  />
            <field name="subtitle" type="string" column="subtitle" length="255" nullable="false"  />
            <field name="pages" type="string" column="pages" length="255" nullable="false"  />
            <field name="usp" type="string" column="usp" length="255" nullable="false"  />
            <one-to-many field="shopOrderBundleArticleCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderBundleArticle" mapped-by="shopOrderItem" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>