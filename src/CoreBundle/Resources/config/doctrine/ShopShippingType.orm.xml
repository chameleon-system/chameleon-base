<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopShippingType" table="shop_shipping_type">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="restrictToArticlesFrom" type="string" column="restrict_to_articles_from" length="255" nullable="false"  />
            <field name="restrictToArticlesTo" type="string" column="restrict_to_articles_to" length="255" nullable="false"  />
            <field name="restrictToWeightFrom" type="string" column="restrict_to_weight_from" length="255" nullable="false"  />
            <field name="restrictToWeightTo" type="string" column="restrict_to_weight_to" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>