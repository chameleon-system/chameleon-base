            <generator strategy="AUTO"/>
        </id>
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="value" column="value" type="string" length="10,2" nullable="false"  />
            <field name="valueType" column="value_type" type="string" length="&#039;absolut&#039;,&#039;prozent&#039;" nullable="false"  />
            <field name="valueBasedOnEntireBasket" column="value_based_on_entire_basket" type="string"  nullable="false"  />
            <field name="valueAdditional" column="value_additional" type="string" length="10,2" nullable="false"  />
            <field name="valueMax" column="value_max" type="string" length="10,2" nullable="false"  />
            <field name="valueMin" column="value_min" type="string" length="10,2" nullable="false"  />
            <field name="addValueForEachArticle" column="add_value_for_each_article" type="string"  nullable="false"  />
            <field name="restrictToSignedInUsers" column="restrict_to_signed_in_users" type="string"  nullable="false"  />
            <field name="applyToAllProducts" column="apply_to_all_products" type="string"  nullable="false"  />
            <field name="endShippingTypeChain" column="end_shipping_type_chain" type="string"  nullable="false"  />
            <field name="position" column="position" type="string"  nullable="false"  />
            <field name="active" column="active" type="string"  nullable="false"  />
            <field name="activeFrom" column="active_from" type="string"  nullable="false"  />
            <field name="activeTo" column="active_to" type="string"  nullable="false"  />
            <field name="restrictToValueFrom" column="restrict_to_value_from" type="string" length="10,2" nullable="false"  />
            <field name="restrictToValueTo" column="restrict_to_value_to" type="string" length="10,2" nullable="false"  />
            <field name="restrictToArticlesFrom" column="restrict_to_articles_from" type="string"  nullable="false"  />
            <field name="restrictToArticlesTo" column="restrict_to_articles_to" type="string"  nullable="false"  />
            <field name="restrictToWeightFrom" column="restrict_to_weight_from" type="string"  nullable="false"  />
            <field name="restrictToWeightTo" column="restrict_to_weight_to" type="string"  nullable="false"  />
