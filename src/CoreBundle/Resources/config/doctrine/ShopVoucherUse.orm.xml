<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopVoucherUse" table="shop_voucher_use">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopVoucherId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopVoucher" inversed-by="shopVoucherUse">
    <join-column name="shop_voucher_id" referenced-column-name="id" />
</many-to-one>
            <field name="dateUsed"  column="date_used"  nullable="false"  />
            <field name="valueUsed"  column="value_used"  nullable="false"  />
            <many-to-one field="shopOrderId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopOrder" inversed-by="shopVoucherUse">
    <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <field name="valueUsedInOrderCurrency"  column="value_used_in_order_currency"  nullable="false"  />
            <field name="pkgShopCurrencyId" type="string" field="pkg_shop_currency_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="pkgShopCurrency" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopCurrency">
    <join-column name="pkg_shop_currency_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>