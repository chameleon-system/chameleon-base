<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopVoucherUse" table="shop_voucher_use">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopVoucher" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVoucher" inversed-by="shopVoucherUseCollection" >
    <join-column name="shop_voucher_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="shopOrder" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrder" inversed-by="shopVoucherUseCollection" >
    <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="pkgShopCurrency" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopCurrency">
    <join-column name="pkg_shop_currency_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>