<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlock" table="pkg_cms_theme_block">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="systemName" type="string" column="system_name" length="255" nullable="false"  />
            <one-to-many field="cmsMasterPagedefSpotCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpot" mapped-by="pkgCmsThemeBlock" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="pkgCmsThemeBlockLayoutCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlockLayout" mapped-by="pkgCmsThemeBlock" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <many-to-one field="pkgCmsThemeBlockLayout" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlockLayout">
    <join-column name="pkg_cms_theme_block_layout_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="cmsMedia" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="cms_media_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>