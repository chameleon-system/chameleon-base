<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlock" table="pkg_cms_theme_block">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name"  column="name"  nullable="false"  />
            <field name="systemName"  column="system_name"  nullable="false"  />
            <one-to-many field="cmsMasterPagedefSpotCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpot" mapped-by="pkgCmsThemeBlockId" />

            <one-to-many field="pkgCmsThemeBlockLayoutCollection" target-entity="ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlockLayout" mapped-by="pkgCmsThemeBlockId" />

            <field name="pkgCmsThemeBlockLayoutId" type="string" field="pkg_cms_theme_block_layout_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="pkgCmsThemeBlockLayout" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlockLayout">
    <join-column name="pkg_cms_theme_block_layout_id" referenced-column-name="id" />
</many-to-one>
            <field name="cmsMediaId" type="string" field="cms_media_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsMedia" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="cms_media_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>