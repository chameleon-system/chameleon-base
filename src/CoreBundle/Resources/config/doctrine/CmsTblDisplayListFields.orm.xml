<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTblDisplayListFields" table="cms_tbl_display_list_fields">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="title" type="string" column="title" length="255" nullable="false"  />
            <many-to-one field="cmsTblConf" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblConf" inversed-by="propertyListFieldsCollection" >
    <join-column name="cms_tbl_conf_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="cmsTranslationFieldName" type="string" column="cms_translation_field_name" length="255" nullable="false"  />
            <field name="dbAlias" type="string" column="db_alias" length="255" nullable="false"  />
            <field name="width" type="string" column="width" length="255" nullable="false"  />
            <field name="callbackFnc" type="string" column="callback_fnc" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>