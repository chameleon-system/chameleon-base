<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTblDisplayListFields" table="cms_tbl_display_list_fields">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="title"  column="title"  nullable="false"  />
            <many-to-one field="cmsTblConfId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsTblConf" inversed-by="propertyListFields">
    <join-column name="cms_tbl_conf_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="cmsTranslationFieldName"  column="cms_translation_field_name"  nullable="false"  />
            <field name="dbAlias"  column="db_alias"  nullable="false"  />
            <field name="position"  column="position"  nullable="false"  />
            <field name="width"  column="width"  nullable="false"  />
            <field name="align"  column="align"  nullable="false"  />
            <field name="callbackFnc"  column="callback_fnc"  nullable="false"  />
            <field name="useCallback"  column="use_callback"  nullable="false"  />
            <field name="showInList"  column="show_in_list"  nullable="false"  />
            <field name="showInSort"  column="show_in_sort"  nullable="false"  />
    </entity>

</doctrine-mapping>