<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopWishlist" table="pkg_shop_wishlist">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="dataExtranetUser" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUser" inversed-by="pkgShopWishlistCollection" >
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <one-to-many field="pkgShopWishlistArticleCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopWishlistArticle" mapped-by="pkgShopWishlist" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="pkgShopWishlistMailHistoryCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopWishlistMailHistory" mapped-by="pkgShopWishlist" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>