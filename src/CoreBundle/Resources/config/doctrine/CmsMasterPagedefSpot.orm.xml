<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpot" table="cms_master_pagedef_spot">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsMasterPagedefId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsMasterPagedef" inversed-by="cmsMasterPagedefSpot">
    <join-column name="cms_master_pagedef_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="pkgCmsThemeBlockId" target-entity="ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlock" inversed-by="cmsMasterPagedefSpot">
    <join-column name="pkg_cms_theme_block_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="model"  column="model"  nullable="false"  />
            <field name="view"  column="view"  nullable="false"  />
            <field name="static"  column="static"  nullable="false"  />
            <one-to-many field="cmsMasterPagedefSpotParameterCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpotParameter" mapped-by="cmsMasterPagedefSpotId" />

            <one-to-many field="cmsMasterPagedefSpotAccessCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpotAccess" mapped-by="cmsMasterPagedefSpotId" />

    </entity>

</doctrine-mapping>