<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpot" table="cms_master_pagedef_spot">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsMasterPagedef" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMasterPagedef" inversed-by="cmsMasterPagedefSpotCollection" >
    <join-column name="cms_master_pagedef_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="pkgCmsThemeBlock" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlock" inversed-by="cmsMasterPagedefSpotCollection" >
    <join-column name="pkg_cms_theme_block_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="model" type="string" column="model" length="255" nullable="false"  />
            <field name="view" type="string" column="view" length="255" nullable="false"  />
            <one-to-many field="cmsMasterPagedefSpotParameterCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpotParameter" mapped-by="cmsMasterPagedefSpot" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="cmsMasterPagedefSpotAccessCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMasterPagedefSpotAccess" mapped-by="cmsMasterPagedefSpot" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>