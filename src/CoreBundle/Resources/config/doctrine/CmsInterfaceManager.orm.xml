<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsInterfaceManager" table="cms_interface_manager">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name"  column="name"  nullable="false"  />
            <field name="systemname"  column="systemname"  nullable="false"  />
            <field name="class"  column="class"  nullable="false"  />
            <field name="classType"  column="class_type"  nullable="false"  />
            <field name="classSubtype"  column="class_subtype"  nullable="false"  />
            <field name="description"  column="description"  nullable="false"  />
            <one-to-many field="cmsInterfaceManagerParameterCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsInterfaceManagerParameter" mapped-by="cmsInterfaceManagerId" />

            <field name="restrictToUserGroups"  column="restrict_to_user_groups"  nullable="false"  />
            <many-to-many field="cmsUsergroupMlt" target-entity="ChameleonSystem\CoreBundle\Entity\CmsUsergroup">
    <join-table name="cms_interface_manager_cms_usergroup_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
    </entity>

</doctrine-mapping>