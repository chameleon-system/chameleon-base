<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgCmsClassManager" table="pkg_cms_class_manager">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="nameOfEntryPoint"  column="name_of_entry_point"  nullable="false"  />
            <field name="exitClass"  column="exit_class"  nullable="false"  />
            <field name="exitClassSubtype"  column="exit_class_subtype"  nullable="false"  />
            <field name="exitClassType"  column="exit_class_type"  nullable="false"  />
            <one-to-many field="pkgCmsClassManagerExtensionCollection" target-entity="ChameleonSystem\CoreBundle\Entity\PkgCmsClassManagerExtension" mapped-by="pkgCmsClassManagerId" />

    </entity>

</doctrine-mapping>