<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopWishlistOrderItem" table="pkg_shop_wishlist_order_item">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="pkgShopWishlist" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopWishlist">
    <join-column name="pkg_shop_wishlist_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="shopOrderItem" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderItem">
    <join-column name="shop_order_item_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="dataExtranetUser" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUser">
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="dataExtranetUserEmail" type="string" column="data_extranet_user_email" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>