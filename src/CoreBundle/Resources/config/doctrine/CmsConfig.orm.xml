<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsConfig" table="cms_config">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <one-to-many field="cmsConfigImagemagickCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsConfigImagemagick" mapped-by="cmsConfig" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="uploaderChunkSize" type="string" column="uploader_chunk_size" length="255" nullable="false"  />
            <one-to-many field="cmsConfigParameterCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsConfigParameter" mapped-by="cmsConfig" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="cmsMessageManagerBackendMessageCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMessageManagerBackendMessage" mapped-by="cmsConfig" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="smtpServer" type="string" column="smtp_server" length="255" nullable="false"  />
            <field name="smtpUser" type="string" column="smtp_user" length="255" nullable="false"  />
            <field name="smtpPassword" type="string" column="smtp_password" length="255" nullable="false"  />
            <field name="smtpPort" type="string" column="smtp_port" length="255" nullable="false"  />
            <one-to-many field="cmsConfigCmsmoduleExtensionsCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsConfigCmsmoduleExtensions" mapped-by="cmsConfig" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="cmsIpWhitelistCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsIpWhitelist" mapped-by="cmsConfig" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="maxDocumentUploadSize" type="string" column="max_document_upload_size" length="255" nullable="false"  />
            <field name="name" type="string" column="name" length="60" nullable="false"  />
            <field name="wysiwygeditorCssUrl" type="string" column="wysiwygeditor_css_url" length="255" nullable="false"  />
            <field name="maxImageUploadSize" type="string" column="max_image_upload_size" length="255" nullable="false"  />
            <field name="updateServer" type="string" column="update_server" length="255" nullable="false"  />
            <field name="entryPerPage" type="string" column="entry_per_page" length="3" nullable="false"  />
            <field name="databaseversion" type="string" column="databaseversion" length="255" nullable="false"  />
            <field name="build" type="string" column="build" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>