<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopVoucher" table="shop_voucher">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopVoucherSeriesId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopVoucherSeries" inversed-by="shopVoucher">
    <join-column name="shop_voucher_series_id" referenced-column-name="id" />
</many-to-one>
            <field name="code"  column="code"  nullable="false"  />
            <field name="datecreated"  column="datecreated"  nullable="false"  />
            <field name="dateUsedUp"  column="date_used_up"  nullable="false"  />
            <field name="isUsedUp"  column="is_used_up"  nullable="false"  />
            <one-to-many field="shopVoucherUseCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopVoucherUse" mapped-by="shopVoucherId" />

    </entity>

</doctrine-mapping>