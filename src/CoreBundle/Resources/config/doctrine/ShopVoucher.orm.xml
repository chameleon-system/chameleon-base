<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopVoucher" table="shop_voucher">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopVoucherSeries" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVoucherSeries" inversed-by="shopVoucherCollection" >
    <join-column name="shop_voucher_series_id" referenced-column-name="id" />
</many-to-one>
            <field name="code" type="string" column="code" length="255" nullable="false"  />
            <one-to-many field="shopVoucherUseCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVoucherUse" mapped-by="shopVoucher" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>