<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsFieldType" table="cms_field_type">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="trans"  column="049_trans"  nullable="false"  />
            <field name="forceAutoIncrement"  column="force_auto_increment"  nullable="false"  />
            <field name="classSubtype"  column="class_subtype"  nullable="false"  />
            <field name="constname"  column="constname"  nullable="false"  />
            <field name="mysqlType"  column="mysql_type"  nullable="false"  />
            <field name="lengthSet"  column="length_set"  nullable="false"  />
            <field name="baseType"  column="base_type"  nullable="false"  />
            <field name="helpText"  column="help_text"  nullable="false"  />
            <field name="mysqlStandardValue"  column="mysql_standard_value"  nullable="false"  />
            <field name="fieldclass"  column="fieldclass"  nullable="false"  />
            <field name="classType"  column="class_type"  nullable="false"  />
            <field name="containsImages"  column="contains_images"  nullable="false"  />
            <field name="indextype"  column="indextype"  nullable="false"  />
    </entity>

</doctrine-mapping>