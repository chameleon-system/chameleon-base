            <field name="helpText" column="help_text" type="string"  nullable="false"  />
            <field name="mysqlStandardValue" column="mysql_standard_value" type="string"  nullable="false"  />
            <field name="fieldclass" column="fieldclass" type="string"  nullable="false"  />
            <field name="classType" column="class_type" type="string" length="&#039;Core&#039;,&#039;Custom-Core&#039;,&#039;Customer&#039;" nullable="false"  />
