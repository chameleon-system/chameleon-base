<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopModuleArticleList" table="shop_module_article_list">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsTplModuleInstanceId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsTplModuleInstance" >
    <join-column name="cms_tpl_module_instance_id" referenced-column-name="id" />
</many-to-one>
            <field name="canBeFiltered"  column="can_be_filtered"  nullable="false"  />
            <field name="name"  column="name"  nullable="false"  />
            <field name="iconId" type="string" field="icon_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="icon" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="icon_id" referenced-column-name="id" />
</many-to-one>
            <field name="shopModuleArticleListFilterId" type="string" field="shop_module_article_list_filter_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopModuleArticleListFilter" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopModuleArticleListFilter">
    <join-column name="shop_module_article_list_filter_id" referenced-column-name="id" />
</many-to-one>
            <field name="shopModuleArticlelistOrderbyId" type="string" field="shop_module_articlelist_orderby_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopModuleArticlelistOrderby" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopModuleArticlelistOrderby">
    <join-column name="shop_module_articlelist_orderby_id" referenced-column-name="id" />
</many-to-one>
            <many-to-many field="shopModuleArticlelistOrderbyMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopModuleArticlelistOrderby">
    <join-table name="shop_module_article_list_shop_module_articlelist_orderby_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="numberOfArticles"  column="number_of_articles"  nullable="false"  />
            <field name="numberOfArticlesPerPage"  column="number_of_articles_per_page"  nullable="false"  />
            <field name="descriptionStart"  column="description_start"  nullable="false"  />
            <field name="descriptionEnd"  column="description_end"  nullable="false"  />
            <many-to-many field="shopArticleGroupMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleGroup">
    <join-table name="shop_module_article_list_shop_article_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopCategoryMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopCategory">
    <join-table name="shop_module_article_list_shop_category_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <one-to-many field="shopModuleArticleListArticleCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopModuleArticleListArticle" mapped-by="shopModuleArticleListId" />

    </entity>

</doctrine-mapping>