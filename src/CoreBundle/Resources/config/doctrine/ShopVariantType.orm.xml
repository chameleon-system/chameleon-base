<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopVariantType" table="shop_variant_type">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopVariantSetId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopVariantSet" inversed-by="shopVariantType">
    <join-column name="shop_variant_set_id" referenced-column-name="id" />
</many-to-one>
            <field name="urlName"  column="url_name"  nullable="false"  />
            <field name="position"  column="position"  nullable="false"  />
            <field name="cmsMediaId" type="string" field="cms_media_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsMedia" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="cms_media_id" referenced-column-name="id" />
</many-to-one>
            <field name="valueSelectType"  column="value_select_type"  nullable="false"  />
            <field name="shopVariantTypeValueCmsfieldname"  column="shop_variant_type_value_cmsfieldname"  nullable="false"  />
            <one-to-many field="shopVariantTypeValueCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopVariantTypeValue" mapped-by="shopVariantTypeId" />

            <field name="name"  column="name"  nullable="false"  />
            <field name="identifier"  column="identifier"  nullable="false"  />
    </entity>

</doctrine-mapping>