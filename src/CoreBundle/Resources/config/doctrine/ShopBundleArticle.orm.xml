<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopBundleArticle" table="shop_bundle_article">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopArticleId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticle" inversed-by="shopBundleArticle">
    <join-column name="shop_article_id" referenced-column-name="id" />
</many-to-one>
            <field name="bundleArticleId" type="string" field="bundle_article_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="bundleArticle" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopArticle">
    <join-column name="bundle_article_id" referenced-column-name="id" />
</many-to-one>
            <field name="amount"  column="amount"  nullable="false"  />
            <field name="position"  column="position"  nullable="false"  />
    </entity>

</doctrine-mapping>