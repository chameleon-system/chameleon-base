<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlockLayout" table="pkg_cms_theme_block_layout">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="pkgCmsThemeBlock" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgCmsThemeBlock" inversed-by="pkgCmsThemeBlockLayoutCollection" >
    <join-column name="pkg_cms_theme_block_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="layoutFile" type="string" column="layout_file" length="255" nullable="false"  />
            <field name="lessFile" type="string" column="less_file" length="255" nullable="false"  />
            <many-to-one field="cmsMedia" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="cms_media_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>