<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\PkgCmsChangelogSet" table="pkg_cms_changelog_set">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="modifyDate" column="modify_date" type="string"  nullable="false"  />
            <field name="cmsUser" column="cms_user" type="string"  nullable="false"  />
            <field name="cmsTblConf" column="cms_tbl_conf" type="string"  nullable="false"  />
            <field name="modifiedId" column="modified_id" type="string"  nullable="false"  />
            <field name="modifiedName" column="modified_name" type="string"  nullable="false"  />
            <field name="changeType" column="change_type" type="string"  nullable="false"  />
            <field name="pkgCmsChangelogItem" column="pkg_cms_changelog_item" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>