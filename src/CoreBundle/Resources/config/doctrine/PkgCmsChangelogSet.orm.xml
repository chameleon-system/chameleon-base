<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgCmsChangelogSet" table="pkg_cms_changelog_set">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsU" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsUser">
    <join-column name="cms_user" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="cmsTblC" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblConf">
    <join-column name="cms_tbl_conf" referenced-column-name="id" />
</many-to-one>
            <field name="modifiedId" type="string" column="modified_id" length="255" nullable="false"  />
            <field name="modifiedName" type="string" column="modified_name" length="255" nullable="false"  />
            <field name="changeType" type="string" column="change_type" length="255" nullable="false"  />
            <one-to-many field="pkgCmsChangelogItemCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgCmsChangelogItem" mapped-by="pkgCmsChangelogSet" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>