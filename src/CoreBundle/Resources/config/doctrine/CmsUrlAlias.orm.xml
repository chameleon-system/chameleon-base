<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsUrlAlias" table="cms_url_alias">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsPortalId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsPortal" inversed-by="cmsUrlAlias">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="sourceUrl"  column="source_url"  nullable="false"  />
            <field name="exactMatch"  column="exact_match"  nullable="false"  />
            <field name="targetUrl"  column="target_url"  nullable="false"  />
            <field name="ignoreParameter"  column="ignore_parameter"  nullable="false"  />
            <field name="parameterMapping"  column="parameter_mapping"  nullable="false"  />
            <field name="cmsUserId" type="string" field="cms_user_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsUser" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsUser">
    <join-column name="cms_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="datecreated"  column="datecreated"  nullable="false">
    <options>
        <option name="default">CURRENT_TIMESTAMP</option>
    </options>
</field>
            <field name="expirationDate"  column="expiration_date"  nullable="false"  />
            <field name="active"  column="active"  nullable="false"  />
    </entity>

</doctrine-mapping>