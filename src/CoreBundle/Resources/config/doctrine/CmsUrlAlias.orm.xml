<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\CmsUrlAlias" table="cms_url_alias">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="cmsPortalId" column="cms_portal_id" type="string"  nullable="false"  />
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="sourceUrl" column="source_url" type="string" length="255" nullable="false"  />
            <field name="exactMatch" column="exact_match" type="string"  nullable="false"  />
            <field name="targetUrl" column="target_url" type="string"  nullable="false"  />
            <field name="ignoreParameter" column="ignore_parameter" type="string"  nullable="false"  />
            <field name="parameterMapping" column="parameter_mapping" type="string"  nullable="false"  />
            <field name="cmsUserId" column="cms_user_id" type="string"  nullable="false"  />
            <field name="datecreated" column="datecreated" type="string"  nullable="false"  />
            <field name="expirationDate" column="expiration_date" type="string"  nullable="false"  />
            <field name="active" column="active" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>