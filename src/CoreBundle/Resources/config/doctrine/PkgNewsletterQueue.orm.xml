<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgNewsletterQueue" table="pkg_newsletter_queue">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="pkgNewsletterU" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgNewsletterUser">
    <join-column name="pkg_newsletter_user" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="pkgNewsletterCampaign" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgNewsletterCampaign" inversed-by="pkgNewsletterQueueCollection" >
    <join-column name="pkg_newsletter_campaign_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>