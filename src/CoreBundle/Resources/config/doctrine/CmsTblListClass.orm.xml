            <field name="classname" type="string" column="classname" length="255" nullable="false"  />
            <many-to-one field="cmsTblConf" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblConf" inversed-by="cmsTblListClassCollection" >
    <join-column name="cms_tbl_conf_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>