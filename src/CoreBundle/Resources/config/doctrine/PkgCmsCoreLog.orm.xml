<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgCmsCoreLog" table="pkg_cms_core_log">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="timestamp"  column="timestamp"  nullable="false"  />
            <field name="channel"  column="channel"  nullable="false"  />
            <field name="level"  column="level"  nullable="false"  />
            <field name="message"  column="message"  nullable="false"  />
            <field name="session"  column="session"  nullable="false"  />
            <field name="uid"  column="uid"  nullable="false"  />
            <field name="file"  column="file"  nullable="false"  />
            <field name="line"  column="line"  nullable="false"  />
            <field name="requestUrl"  column="request_url"  nullable="false"  />
            <field name="referrerUrl"  column="referrer_url"  nullable="false"  />
            <field name="httpMethod"  column="http_method"  nullable="false"  />
            <field name="server"  column="server"  nullable="false"  />
            <field name="ip"  column="ip"  nullable="false"  />
            <field name="dataExtranetUserId" type="string" field="data_extranet_user_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="dataExtranetUser" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUser">
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="dataExtranetUserName"  column="data_extranet_user_name"  nullable="false"  />
            <field name="cmsUserId" type="string" field="cms_user_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsUser" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsUser">
    <join-column name="cms_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="context"  column="context"  nullable="false"  />
    </entity>

</doctrine-mapping>