<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\PkgNewsletterConfirmation" table="pkg_newsletter_confirmation">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="registrationDate" column="registration_date" type="string"  nullable="false"  />
            <field name="confirmation" column="confirmation" type="string"  nullable="false"  />
            <field name="confirmationDate" column="confirmation_date" type="string"  nullable="false"  />
            <field name="pkgNewsletterGroupId" column="pkg_newsletter_group_id" type="string"  nullable="false"  />
            <field name="optoutKey" column="optout_key" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>