<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopUserNoticeList" table="shop_user_notice_list">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="dataExtranetUserId" target-entity="ChameleonSystem\CoreBundle\Entity\DataExtranetUser" inversed-by="shopUserNoticeList">
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="dateAdded"  column="date_added"  nullable="false"  />
            <field name="shopArticleId" type="string" field="shop_article_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopArticle" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopArticle">
    <join-column name="shop_article_id" referenced-column-name="id" />
</many-to-one>
            <field name="amount"  column="amount"  nullable="false"  />
    </entity>

</doctrine-mapping>