<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopAffiliate" table="pkg_shop_affiliate">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shop" target-entity="\ChameleonSystem\CoreBundle\Entity\Shop" inversed-by="pkgShopAffiliateCollection" >
    <join-column name="shop_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="urlParameterName" type="string" column="url_parameter_name" length="255" nullable="false"  />
            <field name="numberOfSecondsValid" type="string" column="number_of_seconds_valid" length="255" nullable="false"  />
            <field name="class" type="string" column="class" length="255" nullable="false"  />
            <field name="classSubtype" type="string" column="class_subtype" length="255" nullable="false"  />
            <one-to-many field="pkgShopAffiliateParameterCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopAffiliateParameter" mapped-by="pkgShopAffiliate" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>