<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopSearchCache" table="shop_search_cache">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopId" target-entity="ChameleonSystem\CoreBundle\Entity\Shop" inversed-by="shopSearchCache">
    <join-column name="shop_id" referenced-column-name="id" />
</many-to-one>
            <field name="searchkey"  column="searchkey"  nullable="false"  />
            <field name="lastUsedDate"  column="last_used_date"  nullable="false"  />
            <one-to-many field="shopSearchCacheItemCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopSearchCacheItem" mapped-by="shopSearchCacheId" />

            <field name="categoryHits"  column="category_hits"  nullable="false"  />
            <field name="numberOfRecordsFound"  column="number_of_records_found"  nullable="false"  />
    </entity>

</doctrine-mapping>