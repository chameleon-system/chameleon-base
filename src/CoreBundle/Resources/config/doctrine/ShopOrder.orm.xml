<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopOrder" table="shop_order">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shop" target-entity="\ChameleonSystem\CoreBundle\Entity\Shop">
    <join-column name="shop_id" referenced-column-name="id" />
</many-to-one>
            <one-to-many field="pkgShopPaymentIpnMessageCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessage" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="amazonPaymentIdMappingCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\AmazonPaymentIdMapping" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="pkgShopPaymentTransactionCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentTransaction" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <many-to-one field="cmsPortal" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsPortal">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="ordernumber" type="string" column="ordernumber" length="255" nullable="false"  />
            <field name="orderIdent" type="string" column="order_ident" length="20" nullable="false"  />
            <one-to-many field="shopOrderItemCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderItem" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <many-to-one field="dataExtranetUser" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUser" inversed-by="shopOrderCollection" >
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="customerNumber" type="string" column="customer_number" length="255" nullable="false"  />
            <field name="userEmail" type="string" column="user_email" length="255" nullable="false"  />
            <field name="adrBillingCompany" type="string" column="adr_billing_company" length="255" nullable="false"  />
            <field name="adrBillingFirstname" type="string" column="adr_billing_firstname" length="255" nullable="false"  />
            <field name="adrBillingLastname" type="string" column="adr_billing_lastname" length="255" nullable="false"  />
            <field name="adrBillingAdditionalInfo" type="string" column="adr_billing_additional_info" length="255" nullable="false"  />
            <field name="adrBillingStreet" type="string" column="adr_billing_street" length="255" nullable="false"  />
            <field name="adrBillingStreetnr" type="string" column="adr_billing_streetnr" length="255" nullable="false"  />
            <field name="adrBillingCity" type="string" column="adr_billing_city" length="255" nullable="false"  />
            <field name="adrBillingPostalcode" type="string" column="adr_billing_postalcode" length="255" nullable="false"  />
            <field name="adrBillingTelefon" type="string" column="adr_billing_telefon" length="255" nullable="false"  />
            <field name="adrBillingFax" type="string" column="adr_billing_fax" length="255" nullable="false"  />
            <field name="userIp" type="string" column="user_ip" length="50" nullable="false"  />
            <field name="adrShippingCompany" type="string" column="adr_shipping_company" length="255" nullable="false"  />
            <field name="adrShippingFirstname" type="string" column="adr_shipping_firstname" length="255" nullable="false"  />
            <field name="adrShippingLastname" type="string" column="adr_shipping_lastname" length="255" nullable="false"  />
            <field name="adrShippingAdditionalInfo" type="string" column="adr_shipping_additional_info" length="255" nullable="false"  />
            <field name="adrShippingStreet" type="string" column="adr_shipping_street" length="255" nullable="false"  />
            <field name="adrShippingStreetnr" type="string" column="adr_shipping_streetnr" length="255" nullable="false"  />
            <field name="adrShippingCity" type="string" column="adr_shipping_city" length="255" nullable="false"  />
            <field name="adrShippingPostalcode" type="string" column="adr_shipping_postalcode" length="255" nullable="false"  />
            <field name="adrShippingTelefon" type="string" column="adr_shipping_telefon" length="255" nullable="false"  />
            <field name="adrShippingFax" type="string" column="adr_shipping_fax" length="255" nullable="false"  />
            <field name="shopShippingGroupName" type="string" column="shop_shipping_group_name" length="255" nullable="false"  />
            <one-to-many field="shopOrderShippingGroupParameterCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderShippingGroupParameter" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="shopPaymentMethodName" type="string" column="shop_payment_method_name" length="255" nullable="false"  />
            <one-to-many field="shopOrderPaymentMethodParameterCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderPaymentMethodParameter" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopOrderVatCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderVat" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="countUniqueArticles" type="string" column="count_unique_articles" length="255" nullable="false"  />
            <field name="affiliateCode" type="string" column="affiliate_code" length="255" nullable="false"  />
            <one-to-many field="shopVoucherUseCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVoucherUse" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopOrderDiscountCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderDiscount" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopOrderStatusCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderStatus" mapped-by="shopOrder" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="vatId" type="string" column="vat_id" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>