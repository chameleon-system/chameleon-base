<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopPrimaryNavi" table="pkg_shop_primary_navi">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsPortalId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsPortal" >
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="active"  column="active"  nullable="false"  />
            <field name="position"  column="position"  nullable="false"  />
            <field name="targetId" type="string" field="target_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="target" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTree">
    <join-column name="target_id" referenced-column-name="id" />
</many-to-one>
            <field name="showRootCategoryTree"  column="show_root_category_tree"  nullable="false"  />
            <field name="cssClass"  column="css_class"  nullable="false"  />
    </entity>

</doctrine-mapping>