<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\DataExtranetUserAddress" table="data_extranet_user_address">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="dataExtranetUserId" target-entity="ChameleonSystem\CoreBundle\Entity\DataExtranetUser" inversed-by="dataExtranetUserAddress">
    <join-column name="data_extranet_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="isDhlPackstation"  column="is_dhl_packstation"  nullable="false"  />
            <field name="company"  column="company"  nullable="false"  />
            <field name="vatId"  column="vat_id"  nullable="false"  />
            <field name="addressAdditionalInfo"  column="address_additional_info"  nullable="false"  />
            <field name="dataExtranetSalutationId" type="string" field="data_extranet_salutation_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="dataExtranetSalutation" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetSalutation">
    <join-column name="data_extranet_salutation_id" referenced-column-name="id" />
</many-to-one>
            <field name="firstname"  column="firstname"  nullable="false"  />
            <field name="lastname"  column="lastname"  nullable="false"  />
            <field name="street"  column="street"  nullable="false"  />
            <field name="streetnr"  column="streetnr"  nullable="false"  />
            <field name="city"  column="city"  nullable="false"  />
            <field name="postalcode"  column="postalcode"  nullable="false"  />
            <field name="dataCountryId" type="string" field="data_country_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="dataCountry" target-entity="\ChameleonSystem\CoreBundle\Entity\DataCountry">
    <join-column name="data_country_id" referenced-column-name="id" />
</many-to-one>
            <field name="telefon"  column="telefon"  nullable="false"  />
            <field name="fax"  column="fax"  nullable="false"  />
    </entity>

</doctrine-mapping>