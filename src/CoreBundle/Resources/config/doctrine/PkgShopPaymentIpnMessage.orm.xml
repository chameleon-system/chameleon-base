<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessage" table="pkg_shop_payment_ipn_message">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <one-to-many field="pkgShopPaymentIpnMessageTriggerCollection" target-entity="ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessageTrigger" mapped-by="pkgShopPaymentIpnMessageId" />

            <field name="cmsPortalId" type="string" field="cms_portal_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsPortal" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsPortal">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="shopOrderId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopOrder" inversed-by="pkgShopPaymentIpnMessage">
    <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="shopPaymentHandlerGroupId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopPaymentHandlerGroup" inversed-by="pkgShopPaymentIpnMessage">
    <join-column name="shop_payment_handler_group_id" referenced-column-name="id" />
</many-to-one>
            <field name="datecreated"  column="datecreated"  nullable="false">
    <options>
        <option name="default">CURRENT_TIMESTAMP</option>
    </options>
</field>
            <field name="pkgShopPaymentIpnStatusId" type="string" field="pkg_shop_payment_ipn_status_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="pkgShopPaymentIpnStatus" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnStatus">
    <join-column name="pkg_shop_payment_ipn_status_id" referenced-column-name="id" />
</many-to-one>
            <field name="success"  column="success"  nullable="false"  />
            <field name="completed"  column="completed"  nullable="false"  />
            <field name="errorType"  column="error_type"  nullable="false"  />
            <field name="ip"  column="ip"  nullable="false"  />
            <field name="requestUrl"  column="request_url"  nullable="false"  />
            <field name="payload"  column="payload"  nullable="false"  />
            <field name="errors"  column="errors"  nullable="false"  />
    </entity>

</doctrine-mapping>