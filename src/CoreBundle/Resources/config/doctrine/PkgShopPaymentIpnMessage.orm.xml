<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessage" table="pkg_shop_payment_ipn_message">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <one-to-many field="pkgShopPaymentIpnMessageTriggerCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessageTrigger" mapped-by="pkgShopPaymentIpnMessage" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <many-to-one field="cmsPortal" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsPortal">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="shopOrder" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrder" inversed-by="pkgShopPaymentIpnMessageCollection" >
    <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="shopPaymentHandlerGroup" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopPaymentHandlerGroup" inversed-by="pkgShopPaymentIpnMessageCollection" >
    <join-column name="shop_payment_handler_group_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="pkgShopPaymentIpnStatus" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnStatus">
    <join-column name="pkg_shop_payment_ipn_status_id" referenced-column-name="id" />
</many-to-one>
            <field name="errorType" type="string" column="error_type" length="255" nullable="false"  />
            <field name="ip" type="string" column="ip" length="255" nullable="false"  />
            <field name="requestUrl" type="string" column="request_url" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>