<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopOrderStep" table="shop_order_step">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="systemname"  column="systemname"  nullable="false"  />
            <field name="urlName"  column="url_name"  nullable="false"  />
            <field name="name"  column="name"  nullable="false"  />
            <field name="showInNavigation"  column="show_in_navigation"  nullable="false"  />
            <field name="description"  column="description"  nullable="false"  />
            <field name="position"  column="position"  nullable="false"  />
            <field name="class"  column="class"  nullable="false"  />
            <field name="classType"  column="class_type"  nullable="false"  />
            <field name="classSubtype"  column="class_subtype"  nullable="false"  />
            <field name="renderViewName"  column="render_view_name"  nullable="false"  />
            <field name="renderViewType"  column="render_view_type"  nullable="false"  />
            <field name="cssIconClassInactive"  column="css_icon_class_inactive"  nullable="false"  />
            <field name="cssIconClassActive"  column="css_icon_class_active"  nullable="false"  />
            <field name="templateNodeCmsTreeId"  column="template_node_cms_tree_id"  nullable="false"  />
    </entity>

</doctrine-mapping>