<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\ShopOrderStep" table="shop_order_step">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="systemname" column="systemname" type="string"  nullable="false"  />
            <field name="urlName" column="url_name" type="string"  nullable="false"  />
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="showInNavigation" column="show_in_navigation" type="string"  nullable="false"  />
            <field name="description" column="description" type="string"  nullable="false"  />
            <field name="position" column="position" type="string"  nullable="false"  />
            <field name="class" column="class" type="string"  nullable="false"  />
            <field name="classType" column="class_type" type="string" length="&#039;Core&#039;,&#039;Custom-Core&#039;,&#039;Customer&#039;" nullable="false"  />
            <field name="classSubtype" column="class_subtype" type="string"  nullable="false"  />
            <field name="renderViewName" column="render_view_name" type="string"  nullable="false"  />
            <field name="renderViewType" column="render_view_type" type="string" length="&#039;Core&#039;,&#039;Custom-Core&#039;,&#039;Customer&#039;" nullable="false"  />
            <field name="cssIconClassInactive" column="css_icon_class_inactive" type="string"  nullable="false"  />
            <field name="cssIconClassActive" column="css_icon_class_active" type="string"  nullable="false"  />
            <field name="templateNodeCmsTreeId" column="template_node_cms_tree_id" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>