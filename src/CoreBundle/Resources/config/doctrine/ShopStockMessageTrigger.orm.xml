<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopStockMessageTrigger" table="shop_stock_message_trigger">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopStockMessage" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopStockMessage" inversed-by="shopStockMessageTriggerCollection" >
    <join-column name="shop_stock_message_id" referenced-column-name="id" />
</many-to-one>
            <field name="amount" type="string" column="amount" length="255" nullable="false"  />
            <field name="message" type="string" column="message" length="255" nullable="false"  />
            <field name="systemName" type="string" column="system_name" length="255" nullable="false"  />
            <field name="cssClass" type="string" column="css_class" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>