<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\CmsFieldConf" table="cms_field_conf">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="cmsTblConfId" column="cms_tbl_conf_id" type="string"  nullable="false"  />
            <field name="name" column="name" type="string" length="80" nullable="false"  />
            <field name="translation" column="translation" type="string" length="80" nullable="false"  />
            <field name="cmsFieldTypeId" column="cms_field_type_id" type="string"  nullable="false"  />
            <field name="cmsTblFieldTab" column="cms_tbl_field_tab" type="string"  nullable="false"  />
            <field name="isrequired" column="isrequired" type="string"  nullable="false"  />
            <field name="fieldclass" column="fieldclass" type="string"  nullable="false"  />
            <field name="fieldclassSubtype" column="fieldclass_subtype" type="string"  nullable="false"  />
            <field name="classType" column="class_type" type="string" length="&#039;Core&#039;,&#039;Custom-Core&#039;,&#039;Customer&#039;" nullable="false"  />
            <field name="modifier" column="modifier" type="string" length="&#039;none&#039;,&#039;readonly&#039;,&#039;hidden&#039;,&#039;edit-on-click&#039;,&#039;readonly-if-filled&#039;" nullable="false"  />
            <field name="fieldDefaultValue" column="field_default_value" type="string" length="255" nullable="false"  />
            <field name="lengthSet" column="length_set" type="string"  nullable="false"  />
            <field name="fieldtypeConfig" column="fieldtype_config" type="string"  nullable="false"  />
            <field name="restrictToGroups" column="restrict_to_groups" type="string"  nullable="false"  />
            <field name="cmsUsergroupMlt" column="cms_usergroup_mlt" type="string"  nullable="false"  />
            <field name="fieldWidth" column="field_width" type="string" length="11" nullable="false"  />
            <field name="position" column="position" type="string"  nullable="false"  />
            <field name="049Helptext" column="049_helptext" type="string"  nullable="false"  />
            <field name="rowHexcolor" column="row_hexcolor" type="string" length="6" nullable="false"  />
            <field name="isTranslatable" column="is_translatable" type="string"  nullable="false"  />
            <field name="validationRegex" column="validation_regex" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>