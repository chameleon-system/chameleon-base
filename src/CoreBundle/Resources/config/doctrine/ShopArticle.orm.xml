        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopAttributeValueMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopAttributeValue">
    <join-table name="shop_article_shop_attribute_value_mlt">
        <join-columns>
