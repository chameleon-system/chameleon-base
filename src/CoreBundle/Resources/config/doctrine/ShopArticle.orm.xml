        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopMlt" target-entity="ChameleonSystem\CoreBundle\Entity\Shop">
    <join-table name="shop_article_shop_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <one-to-many field="shopArticleContributorCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleContributor" mapped-by="shopArticleId" />

            <field name="subtitle"  column="subtitle"  nullable="false"  />
            <field name="isNew"  column="is_new"  nullable="false"  />
            <field name="usp"  column="usp"  nullable="false"  />
            <field name="stars"  column="stars"  nullable="false"  />
            <one-to-many field="shopArticleReviewCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleReview" mapped-by="shopArticleId" />

            <field name="isBundle"  column="is_bundle"  nullable="false"  />
            <one-to-many field="shopBundleArticleCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopBundleArticle" mapped-by="shopArticleId" />

            <many-to-many field="download" target-entity="ChameleonSystem\CoreBundle\Entity\CmsDocument">
    <join-table name="shop_article_download_cms_document_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="nameVariantInfo"  column="name_variant_info"  nullable="false"  />
            <field name="shopVariantSetId" type="string" field="shop_variant_set_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopVariantSet" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVariantSet">
    <join-column name="shop_variant_set_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="variantParentId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticle" inversed-by="shopArticleVariants">
    <join-column name="variant_parent_id" referenced-column-name="id" />
</many-to-one>
            <field name="variantParentIsActive"  column="variant_parent_is_active"  nullable="false"  />
            <one-to-many field="shopArticleVariantsCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticle" mapped-by="variantParentId" />

            <many-to-many field="shopVariantTypeValueMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopVariantTypeValue">
    <join-table name="shop_article_shop_variant_type_value_mlt">
        <join-columns>
