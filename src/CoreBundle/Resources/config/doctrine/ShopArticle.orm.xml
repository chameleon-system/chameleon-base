<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopArticle" table="shop_article">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name"  column="name"  nullable="false"  />
            <many-to-many field="shopArticle2Mlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticle">
    <join-table name="shop_article_shop_article2_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="seoPattern"  column="seo_pattern"  nullable="false"  />
            <field name="articlenumber"  column="articlenumber"  nullable="false"  />
            <one-to-many field="shopArticleImageCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleImage" mapped-by="shopArticleId" />

            <field name="cmsMediaDefaultPreviewImageId" type="string" field="cms_media_default_preview_image_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsMediaDefaultPreviewImage" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="cms_media_default_preview_image_id" referenced-column-name="id" />
</many-to-one>
            <one-to-many field="shopArticlePreviewImageCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticlePreviewImage" mapped-by="shopArticleId" />

            <one-to-many field="shopArticleDocumentCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleDocument" mapped-by="shopArticleId" />

            <field name="listRank"  column="list_rank"  nullable="false"  />
            <field name="datecreated"  column="datecreated"  nullable="false">
    <options>
        <option name="default">CURRENT_TIMESTAMP</option>
    </options>
</field>
            <field name="active"  column="active"  nullable="false"  />
            <field name="descriptionShort"  column="description_short"  nullable="false"  />
            <field name="description"  column="description"  nullable="false"  />
            <field name="shopManufacturerId" type="string" field="shop_manufacturer_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopManufacturer" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopManufacturer">
    <join-column name="shop_manufacturer_id" referenced-column-name="id" />
</many-to-one>
            <field name="price"  column="price"  nullable="false"  />
            <field name="priceReference"  column="price_reference"  nullable="false"  />
            <field name="shopVatId" type="string" field="shop_vat_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopVat" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVat">
    <join-column name="shop_vat_id" referenced-column-name="id" />
</many-to-one>
            <field name="sizeWeight"  column="size_weight"  nullable="false"  />
            <field name="sizeWidth"  column="size_width"  nullable="false"  />
            <field name="sizeHeight"  column="size_height"  nullable="false"  />
            <field name="sizeLength"  column="size_length"  nullable="false"  />
            <field name="quantityInUnits"  column="quantity_in_units"  nullable="false"  />
            <field name="shopUnitOfMeasurementId" type="string" field="shop_unit_of_measurement_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopUnitOfMeasurement" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopUnitOfMeasurement">
    <join-column name="shop_unit_of_measurement_id" referenced-column-name="id" />
</many-to-one>
            <one-to-many field="shopArticleStockCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleStock" mapped-by="shopArticleId" />

            <field name="showPreorderOnZeroStock"  column="show_preorder_on_zero_stock"  nullable="false"  />
            <field name="shopStockMessageId" type="string" field="shop_stock_message_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopStockMessage" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopStockMessage">
    <join-column name="shop_stock_message_id" referenced-column-name="id" />
</many-to-one>
            <field name="virtualArticle"  column="virtual_article"  nullable="false"  />
            <field name="isSearchable"  column="is_searchable"  nullable="false"  />
            <field name="excludeFromShippingCostCalculation"  column="exclude_from_shipping_cost_calculation"  nullable="false"  />
            <field name="excludeFromVouchers"  column="exclude_from_vouchers"  nullable="false"  />
            <field name="excludeFromDiscounts"  column="exclude_from_discounts"  nullable="false"  />
