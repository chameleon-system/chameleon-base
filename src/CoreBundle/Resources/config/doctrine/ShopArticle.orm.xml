    <join-table name="shop_article_shop_article_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopArticleTypeMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleType">
    <join-table name="shop_article_shop_article_type_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopCategoryMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopCategory">
    <join-table name="shop_article_shop_category_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="shopCategoryId" type="string" field="shop_category_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopCategory" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopCategory">
    <join-column name="shop_category_id" referenced-column-name="id" />
</many-to-one>
            <many-to-many field="shopArticleMarkerMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleMarker">
    <join-table name="shop_article_shop_article_marker_mlt">
        <join-columns>
