            <field name="metaDescription"  column="meta_description"  nullable="false"  />
            <many-to-many field="shopArticleMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticle">
    <join-table name="shop_article_shop_article_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
