<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopListfilterItem" table="pkg_shop_listfilter_item">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="pkgShopListfilter" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopListfilter" inversed-by="pkgShopListfilterItemCollection" >
    <join-column name="pkg_shop_listfilter_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="pkgShopListfilterItemT" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopListfilterItemType">
    <join-column name="pkg_shop_listfilter_item_type" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="systemname" type="string" column="systemname" length="255" nullable="false"  />
            <many-to-one field="shopAttrib" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopAttribute">
    <join-column name="shop_attribute" referenced-column-name="id" />
</many-to-one>
            <field name="previewSize" type="string" column="preview_size" length="255" nullable="false"  />
            <field name="minValue" type="string" column="min_value" length="255" nullable="false"  />
            <field name="maxValue" type="string" column="max_value" length="255" nullable="false"  />
            <field name="mysqlFieldName" type="string" column="mysql_field_name" length="255" nullable="false"  />
            <field name="view" type="string" column="view" length="255" nullable="false"  />
            <field name="variantIdentifier" type="string" column="variant_identifier" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>