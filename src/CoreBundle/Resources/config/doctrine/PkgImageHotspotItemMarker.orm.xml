            <field name="left" column="left" type="string"  nullable="false"  />
            <field name="linkedRecord" column="linked_record" type="string"  nullable="false"  />
            <field name="url" column="url" type="string"  nullable="false"  />
            <field name="showObjectLayover" column="show_object_layover" type="string"  nullable="false"  />
            <field name="cmsMediaId" column="cms_media_id" type="string"  nullable="false"  />
