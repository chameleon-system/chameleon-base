<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\ShopOrderStatusCode" table="shop_order_status_code">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="shopId" column="shop_id" type="string"  nullable="false"  />
            <field name="sendMailNotification" column="send_mail_notification" type="string"  nullable="false"  />
            <field name="systemName" column="system_name" type="string"  nullable="false"  />
            <field name="pkgShopPaymentTransactionTypeId" column="pkg_shop_payment_transaction_type_id" type="string"  nullable="false"  />
            <field name="dataMailProfileId" column="data_mail_profile_id" type="string"  nullable="false"  />
            <field name="infoText" column="info_text" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>