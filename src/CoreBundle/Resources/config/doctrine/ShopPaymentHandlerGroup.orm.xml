<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopPaymentHandlerGroup" table="shop_payment_handler_group">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="classname" type="string" column="classname" length="255" nullable="false"  />
            <one-to-many field="shopPaymentHandlerGroupConfigCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopPaymentHandlerGroupConfig" mapped-by="shopPaymentHandlerGroup" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="ipnGroupIdentifier" type="string" column="ipn_group_identifier" length="255" nullable="false"  />
            <field name="ipnPayloadCharacterCharset" type="string" column="ipn_payload_character_charset" length="255" nullable="false"  />
            <one-to-many field="pkgShopPaymentIpnStatusCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnStatus" mapped-by="shopPaymentHandlerGroup" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="systemName" type="string" column="system_name" length="255" nullable="false"  />
            <one-to-many field="shopPaymentHandlerCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopPaymentHandler" mapped-by="shopPaymentHandlerGroup" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopPaymentMethodCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopPaymentMethod" mapped-by="shopPaymentHandlerGroup" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="pkgShopPaymentIpnTriggerCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnTrigger" mapped-by="shopPaymentHandlerGroup" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="pkgShopPaymentIpnMessageCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessage" mapped-by="shopPaymentHandlerGroup" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>