<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopOrderBasket" table="shop_order_basket">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="orderIdent"  column="order_ident"  nullable="false"  />
            <field name="sessionId"  column="session_id"  nullable="false"  />
            <field name="datecreated"  column="datecreated"  nullable="false"  />
            <field name="lastmodified"  column="lastmodified"  nullable="false"  />
            <field name="rawdataBasket"  column="rawdata_basket"  nullable="false"  />
            <field name="rawdataUser"  column="rawdata_user"  nullable="false"  />
            <field name="rawdataSession"  column="rawdata_session"  nullable="false"  />
            <field name="shopOrderId" type="string" field="shop_order_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopOrder" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrder">
    <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <field name="updateStepname"  column="update_stepname"  nullable="false"  />
            <field name="processed"  column="processed"  nullable="false"  />
    </entity>

</doctrine-mapping>