<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsMessageManagerMessage" table="cms_message_manager_message">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsPortal" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsPortal" inversed-by="cmsMessageManagerMessageCollection" >
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <many-to-one field="cmsMessageManagerMessageType" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMessageManagerMessageType">
    <join-column name="cms_message_manager_message_type_id" referenced-column-name="id" />
</many-to-one>
            <field name="messageView" type="string" column="message_view" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>