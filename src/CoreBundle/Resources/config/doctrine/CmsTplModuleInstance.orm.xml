<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTplModuleInstance" table="cms_tpl_module_instance">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name" type="string" column="name" length="100" nullable="false"  />
            <one-to-many field="cmsTplPageCmsMasterPagedefSpotCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTplPageCmsMasterPagedefSpot" mapped-by="cmsTplModuleInstance" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <many-to-one field="cmsPortal" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsPortal">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="template" type="string" column="template" length="255" nullable="false"  />
            <many-to-one field="cmsTplModule" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTplModule">
    <join-column name="cms_tpl_module_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>