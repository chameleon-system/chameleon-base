<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopCategory" table="shop_category">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopCategory" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopCategory" inversed-by="shopCategoryCollection" >
    <join-column name="shop_category_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="nameProduct" type="string" column="name_product" length="255" nullable="false"  />
            <field name="seoPattern" type="string" column="seo_pattern" length="255" nullable="false"  />
            <one-to-many field="shopCategoryCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopCategory" mapped-by="shopCategory" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="metaKeywords" type="string" column="meta_keywords" length="255" nullable="false"  />
            <one-to-many field="shopCategoryTabCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopCategoryTab" mapped-by="shopCategory" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="metaDescription" type="string" column="meta_description" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>