        <option name="default"></option>
    </options>
</field>
<many-to-one field="defaultPkgShopCurrency" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopCurrency">
    <join-column name="default_pkg_shop_currency_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
