<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\Shop" table="shop">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <one-to-many field="shopOrderStatusCodeCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopOrderStatusCode" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="adrCompany" type="string" column="adr_company" length="255" nullable="false"  />
            <field name="adrStreet" type="string" column="adr_street" length="255" nullable="false"  />
            <field name="adrZip" type="string" column="adr_zip" length="255" nullable="false"  />
            <field name="adrCity" type="string" column="adr_city" length="255" nullable="false"  />
            <field name="customerServiceTelephone" type="string" column="customer_service_telephone" length="255" nullable="false"  />
            <field name="customerServiceEmail" type="string" column="customer_service_email" length="255" nullable="false"  />
            <field name="shopvatnumber" type="string" column="shopvatnumber" length="255" nullable="false"  />
            <one-to-many field="shopBankAccountCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopBankAccount" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="dataExtranetUserCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\DataExtranetUser" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="dataExtranetUserShopArticleHistoryMaxArticleCount" type="string" column="data_extranet_user_shop_article_history_max_article_count" length="255" nullable="false"  />
            <field name="affiliateParameterName" type="string" column="affiliate_parameter_name" length="255" nullable="false"  />
            <one-to-many field="pkgShopAffiliateCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopAffiliate" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopArticleImageSizeCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopArticleImageSize" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopSystemInfoCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSystemInfo" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="shopSearchMinIndexLength" type="string" column="shop_search_min_index_length" length="255" nullable="false"  />
            <field name="shopSearchMaxIndexLength" type="string" column="shop_search_max_index_length" length="255" nullable="false"  />
            <field name="maxSearchCacheAgeInHours" type="string" column="max_search_cache_age_in_hours" length="255" nullable="false"  />
            <one-to-many field="shopSearchLogCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSearchLog" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopSearchFieldWeightCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSearchFieldWeight" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopSearchIgnoreWordCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSearchIgnoreWord" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopSearchKeywordArticleCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSearchKeywordArticle" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="shopSearchCacheCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopSearchCache" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="basketSpotName" type="string" column="basket_spot_name" length="255" nullable="false"  />
            <field name="shopCentralHandlerSpotName" type="string" column="shop_central_handler_spot_name" length="255" nullable="false"  />
            <one-to-many field="shopStockMessageCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopStockMessage" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="exportKey" type="string" column="export_key" length="255" nullable="false"  />
            <field name="dataExtranetUserShopArticleHistoryMaxCookieSize" type="string" column="data_extranet_user_shop_article_history_max_cookie_size" length="255" nullable="false"  />
            <field name="shopreviewmailMailDelay" type="string" column="shopreviewmail_mail_delay" length="255" nullable="false"  />
            <one-to-many field="pkgShopFooterCategoryCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgShopFooterCategory" mapped-by="shop" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>