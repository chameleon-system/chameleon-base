<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTplPage" table="cms_tpl_page">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="treePathSearchString"  column="tree_path_search_string"  nullable="false"  />
            <field name="cmsMasterPagedefId" type="string" field="cms_master_pagedef_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsMasterPagedef" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMasterPagedef">
    <join-column name="cms_master_pagedef_id" referenced-column-name="id" />
</many-to-one>
            <one-to-many field="cmsTplPageCmsMasterPagedefSpotCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsTplPageCmsMasterPagedefSpot" mapped-by="cmsTplPageId" />

            <field name="name"  column="name"  nullable="false"  />
            <field name="seoPattern"  column="seo_pattern"  nullable="false"  />
            <field name="cmsPortalId" type="string" field="cms_portal_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsPortal" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsPortal">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="primaryTreeIdHidden"  column="primary_tree_id_hidden"  nullable="false"  />
            <field type="array" name="images" column="images"  nullable="false"  />
            <field name="backgroundImageId" type="string" field="background_image_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="backgroundImage" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="background_image_id" referenced-column-name="id" />
</many-to-one>
            <many-to-many field="cmsUsergroupMlt" target-entity="ChameleonSystem\CoreBundle\Entity\CmsUsergroup">
    <join-table name="cms_tpl_page_cms_usergroup_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="cmsUserId" type="string" field="cms_user_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsUser" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsUser">
    <join-column name="cms_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="usessl"  column="usessl"  nullable="false"  />
            <field name="extranetPage"  column="extranet_page"  nullable="false"  />
            <field name="accessNotConfirmedUser"  column="access_not_confirmed_user"  nullable="false"  />
            <many-to-many field="dataExtranetGroupMlt" target-entity="ChameleonSystem\CoreBundle\Entity\DataExtranetGroup">
    <join-table name="cms_tpl_page_data_extranet_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="ivwCode"  column="ivw_code"  nullable="false"  />
            <field name="cmsLanguageId" type="string" field="cms_language_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsLanguage" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsLanguage">
    <join-column name="cms_language_id" referenced-column-name="id" />
</many-to-one>
            <field name="metaDescription"  column="meta_description"  nullable="false"  />
            <field name="metaKeywords"  column="meta_keywords"  nullable="false"  />
            <field name="metaKeywordLanguage"  column="meta_keyword_language"  nullable="false"  />
            <field name="metaAuthor"  column="meta_author"  nullable="false"  />
            <field name="metaPublisher"  column="meta_publisher"  nullable="false"  />
            <field name="metaPageTopic"  column="meta_page_topic"  nullable="false"  />
            <field name="metaPragma"  column="meta_pragma"  nullable="false"  />
            <field name="metaRobots"  column="meta_robots"  nullable="false"  />
            <field name="metaRevisitAfter"  column="meta_revisit_after"  nullable="false"  />
    </entity>

</doctrine-mapping>