<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\CmsTree" table="cms_tree">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="parentId" column="parent_id" type="string"  nullable="false"  />
            <field name="lft" column="lft" type="string"  nullable="false"  />
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="rgt" column="rgt" type="string"  nullable="false"  />
            <field name="urlname" column="urlname" type="string"  nullable="false"  />
            <field name="hidden" column="hidden" type="string"  nullable="false"  />
            <field name="showExtranetPage" column="show_extranet_page" type="string"  nullable="false"  />
            <field name="entrySort" column="entry_sort" type="string"  nullable="false"  />
            <field name="link" column="link" type="string"  nullable="false"  />
            <field name="linkTarget" column="linkTarget" type="string"  nullable="false"  />
            <field name="cmsTplPagePrimaryLink" column="cms_tpl_page_primary_link" type="string"  nullable="false"  />
            <field name="naviIconCmsMediaId" column="navi_icon_cms_media_id" type="string"  nullable="false"  />
            <field name="pathcache" column="pathcache" type="string" length="512" nullable="false"  />
            <field name="cmsTplModuleInstanceId" column="cms_tpl_module_instance_id" type="string"  nullable="false"  />
            <field name="seoNofollow" column="seo_nofollow" type="string"  nullable="false"  />
            <field name="cmsTplPageMlt" column="cms_tpl_page_mlt" type="string"  nullable="false"  />
            <field name="htmlAccessKey" column="html_access_key" type="string" length="1" nullable="false"  />
            <field name="cssClasses" column="css_classes" type="string"  nullable="false"  />
            <field name="cmsTreeNode" column="cms_tree_node" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>