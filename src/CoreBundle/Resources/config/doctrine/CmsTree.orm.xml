<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTree" table="cms_tree">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="lft" type="string" column="lft" length="255" nullable="false"  />
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="rgt" type="string" column="rgt" length="255" nullable="false"  />
            <field name="urlname" type="string" column="urlname" length="255" nullable="false"  />
            <field name="entrySort" type="string" column="entry_sort" length="255" nullable="false"  />
            <field name="link" type="string" column="link" length="255" nullable="false"  />
            <field name="cmsTplPagePrimaryLink" type="string" column="cms_tpl_page_primary_link" length="255" nullable="false"  />
            <field name="htmlAccessKey" type="string" column="html_access_key" length="1" nullable="false"  />
            <field name="cssClasses" type="string" column="css_classes" length="255" nullable="false"  />
            <one-to-many field="cmsTreeNodeCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTreeNode" mapped-by="cmsTree" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>