<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsConfigImagemagick" table="cms_config_imagemagick">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsConfigId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsConfig" inversed-by="cmsConfigImagemagick">
    <join-column name="cms_config_id" referenced-column-name="id" />
</many-to-one>
            <field name="fromImageSize"  column="from_image_size"  nullable="false"  />
            <field name="forceJpeg"  column="force_jpeg"  nullable="false"  />
            <field name="quality"  column="quality"  nullable="false"  />
            <field name="scharpen"  column="scharpen"  nullable="false"  />
            <field name="gamma"  column="gamma"  nullable="false"  />
    </entity>

</doctrine-mapping>