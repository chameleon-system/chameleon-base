<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\CmsContentBox" table="cms_content_box">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="className" column="class_name" type="string"  nullable="false"  />
            <field name="classType" column="class_type" type="string" length="&#039;Core&#039;,&#039;Custom-Core&#039;,&#039;Customer&#039;" nullable="false"  />
            <field name="classPath" column="class_path" type="string"  nullable="false"  />
            <field name="systemName" column="system_name" type="string"  nullable="false"  />
            <field name="name" column="name" type="string" length="60" nullable="false"  />
            <field name="headlinecolHexcolor" column="headlinecol_hexcolor" type="string"  nullable="false"  />
            <field name="iconList" column="icon_list" type="string" length="60" nullable="false"  />
            <field name="showInCol" column="show_in_col" type="string" length="&#039;left&#039;,&#039;middle&#039;,&#039;right&#039;" nullable="false"  />
    </entity>

</doctrine-mapping>