<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgImageHotspotItem" table="pkg_image_hotspot_item">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="pkgImageHotspot" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgImageHotspot" inversed-by="pkgImageHotspotItemCollection" >
    <join-column name="pkg_image_hotspot_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <one-to-many field="pkgImageHotspotItemSpotCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgImageHotspotItemSpot" mapped-by="pkgImageHotspotItem" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="pkgImageHotspotItemMarkerCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgImageHotspotItemMarker" mapped-by="pkgImageHotspotItem" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

    </entity>

</doctrine-mapping>