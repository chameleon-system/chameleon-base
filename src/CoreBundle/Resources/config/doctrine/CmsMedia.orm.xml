<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\CmsMedia" table="cms_media">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="height" column="height" type="string" length="16" nullable="false"  />
            <field name="cmsFiletypeId" column="cms_filetype_id" type="string"  nullable="false"  />
            <field name="filesize" column="filesize" type="string"  nullable="false"  />
            <field name="cmsMediaTreeId" column="cms_media_tree_id" type="string"  nullable="false"  />
            <field name="width" column="width" type="string" length="16" nullable="false"  />
            <field name="description" column="description" type="string" length="255" nullable="false"  />
            <field name="metatags" column="metatags" type="string" length="255" nullable="false"  />
            <field name="filetypes" column="filetypes" type="string" length="100" nullable="false"  />
            <field name="altTag" column="alt_tag" type="string"  nullable="false"  />
            <field name="systemname" column="systemname" type="string"  nullable="false"  />
            <field name="cmsTagsMlt" column="cms_tags_mlt" type="string"  nullable="false"  />
            <field name="customFilename" column="custom_filename" type="string" length="200" nullable="false"  />
            <field name="path" column="path" type="string" length="255" nullable="false"  />
            <field name="cmsMediaId" column="cms_media_id" type="string"  nullable="false"  />
            <field name="externalEmbedCode" column="external_embed_code" type="string"  nullable="false"  />
            <field name="externalVideoThumbnail" column="external_video_thumbnail" type="string"  nullable="false"  />
            <field name="timeStamp" column="time_stamp" type="string"  nullable="false"  />
            <field name="dateChanged" column="date_changed" type="string"  nullable="false"  />
            <field name="refreshToken" column="refresh_token" type="string" length="8" nullable="false"  />
            <field name="cmsUserId" column="cms_user_id" type="string"  nullable="false"  />
            <field name="externalVideoId" column="external_video_id" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>