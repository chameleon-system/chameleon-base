<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsMedia" table="cms_media">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="height"  column="height"  nullable="false"  />
            <field name="cmsFiletypeId" type="string" field="cms_filetype_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsFiletype" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsFiletype">
    <join-column name="cms_filetype_id" referenced-column-name="id" />
</many-to-one>
            <field name="filesize"  column="filesize"  nullable="false"  />
            <field name="cmsMediaTreeId"  column="cms_media_tree_id"  nullable="false"  />
            <field name="width"  column="width"  nullable="false"  />
            <field name="description"  column="description"  nullable="false"  />
            <field name="metatags"  column="metatags"  nullable="false"  />
            <field name="filetypes"  column="filetypes"  nullable="false"  />
            <field name="altTag"  column="alt_tag"  nullable="false"  />
            <field name="systemname"  column="systemname"  nullable="false"  />
            <many-to-many field="cmsTagsMlt" target-entity="ChameleonSystem\CoreBundle\Entity\CmsTags">
    <join-table name="cms_media_cms_tags_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="customFilename"  column="custom_filename"  nullable="false"  />
            <field name="path"  column="path"  nullable="false"  />
            <field name="cmsMediaId" type="string" field="cms_media_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsMedia" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="cms_media_id" referenced-column-name="id" />
</many-to-one>
            <field name="externalEmbedCode"  column="external_embed_code"  nullable="false"  />
            <field name="externalVideoThumbnail"  column="external_video_thumbnail"  nullable="false"  />
            <field name="timeStamp"  column="time_stamp"  nullable="false"  />
            <field name="dateChanged"  column="date_changed"  nullable="false">
    <options>
        <option name="default">CURRENT_TIMESTAMP</option>
    </options>
</field>
            <field name="refreshToken"  column="refresh_token"  nullable="false"  />
            <field name="cmsUserId" type="string" field="cms_user_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsUser" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsUser">
    <join-column name="cms_user_id" referenced-column-name="id" />
</many-to-one>
            <field name="externalVideoId"  column="external_video_id"  nullable="false"  />
    </entity>

</doctrine-mapping>