<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\ShopShippingGroup" table="shop_shipping_group">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="shopShippingGroupHandlerId" column="shop_shipping_group_handler_id" type="string"  nullable="false"  />
            <field name="position" column="position" type="string"  nullable="false"  />
            <field name="active" column="active" type="string"  nullable="false"  />
            <field name="activeFrom" column="active_from" type="string"  nullable="false"  />
            <field name="activeTo" column="active_to" type="string"  nullable="false"  />
            <field name="dataExtranetUserMlt" column="data_extranet_user_mlt" type="string"  nullable="false"  />
            <field name="dataExtranetGroupMlt" column="data_extranet_group_mlt" type="string"  nullable="false"  />
            <field name="shopVatId" column="shop_vat_id" type="string"  nullable="false"  />
            <field name="shopShippingTypeMlt" column="shop_shipping_type_mlt" type="string"  nullable="false"  />
            <field name="shopPaymentMethodMlt" column="shop_payment_method_mlt" type="string"  nullable="false"  />
            <field name="shopShippingGroupMlt" column="shop_shipping_group_mlt" type="string"  nullable="false"  />
            <field name="cmsPortalMlt" column="cms_portal_mlt" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>