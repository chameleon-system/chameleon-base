<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopOrderDiscount" table="shop_order_discount">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopOrderId" target-entity="ChameleonSystem\CoreBundle\Entity\ShopOrder" inversed-by="shopOrderDiscount">
    <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <field name="shopDiscountId"  column="shop_discount_id"  nullable="false"  />
            <field name="name"  column="name"  nullable="false"  />
            <field name="value"  column="value"  nullable="false"  />
            <field name="valuetype"  column="valuetype"  nullable="false"  />
            <field name="freearticleName"  column="freearticle_name"  nullable="false"  />
            <field name="freearticleArticlenumber"  column="freearticle_articlenumber"  nullable="false"  />
            <field name="freearticleId"  column="freearticle_id"  nullable="false"  />
            <field name="total"  column="total"  nullable="false"  />
    </entity>

</doctrine-mapping>