<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsMenuItem" table="cms_menu_item">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <many-to-one field="tar" target-entity="\ChameleonSystem\AutoclassesBundle\TableConfExport\AbstractEntity">
    <join-column name="target" referenced-column-name="id" />
</many-to-one>
            <field name="iconFontCssClass" type="string" column="icon_font_css_class" length="255" nullable="false"  />
            <many-to-one field="cmsMenuCategory" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMenuCategory" inversed-by="cmsMenuItemCollection" >
    <join-column name="cms_menu_category_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>