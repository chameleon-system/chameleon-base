<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopArticleImageSize" table="shop_article_image_size">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shop" target-entity="\ChameleonSystem\CoreBundle\Entity\Shop" inversed-by="shopArticleImageSizeCollection" >
    <join-column name="shop_id" referenced-column-name="id" />
</many-to-one>
            <field name="nameInternal" type="string" column="name_internal" length="255" nullable="false"  />
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="width" type="string" column="width" length="255" nullable="false"  />
            <field name="height" type="string" column="height" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>