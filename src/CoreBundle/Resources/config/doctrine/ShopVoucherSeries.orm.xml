<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopVoucherSeries" table="shop_voucher_series">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name"  column="name"  nullable="false"  />
            <field name="shopVoucherSeriesSponsorId" type="string" field="shop_voucher_series_sponsor_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopVoucherSeriesSponsor" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVoucherSeriesSponsor">
    <join-column name="shop_voucher_series_sponsor_id" referenced-column-name="id" />
</many-to-one>
            <field name="value"  column="value"  nullable="false"  />
            <field name="valueType"  column="value_type"  nullable="false"  />
            <field name="shopVatId" type="string" field="shop_vat_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="shopVat" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopVat">
    <join-column name="shop_vat_id" referenced-column-name="id" />
</many-to-one>
            <field name="freeShipping"  column="free_shipping"  nullable="false"  />
            <field name="active"  column="active"  nullable="false"  />
            <field name="activeFrom"  column="active_from"  nullable="false"  />
            <field name="activeTo"  column="active_to"  nullable="false"  />
            <field name="restrictToValue"  column="restrict_to_value"  nullable="false"  />
            <field name="restrictToOtherSeries"  column="restrict_to_other_series"  nullable="false"  />
            <field name="allowNoOtherVouchers"  column="allow_no_other_vouchers"  nullable="false"  />
            <field name="restrictToOnePerUser"  column="restrict_to_one_per_user"  nullable="false"  />
            <field name="restrictToFirstOrder"  column="restrict_to_first_order"  nullable="false"  />
            <many-to-many field="dataExtranetUserMlt" target-entity="ChameleonSystem\CoreBundle\Entity\DataExtranetUser">
    <join-table name="shop_voucher_series_data_extranet_user_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="dataExtranetGroupMlt" target-entity="ChameleonSystem\CoreBundle\Entity\DataExtranetGroup">
    <join-table name="shop_voucher_series_data_extranet_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopManufacturerMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopManufacturer">
    <join-table name="shop_voucher_series_shop_manufacturer_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopArticleGroupMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticleGroup">
    <join-table name="shop_voucher_series_shop_article_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopCategoryMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopCategory">
    <join-table name="shop_voucher_series_shop_category_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <many-to-many field="shopArticleMlt" target-entity="ChameleonSystem\CoreBundle\Entity\ShopArticle">
    <join-table name="shop_voucher_series_shop_article_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <one-to-many field="shopVoucherCollection" target-entity="ChameleonSystem\CoreBundle\Entity\ShopVoucher" mapped-by="shopVoucherSeriesId" />

    </entity>

</doctrine-mapping>