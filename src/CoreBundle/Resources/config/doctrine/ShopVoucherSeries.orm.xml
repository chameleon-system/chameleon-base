<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="\ChameleonSystem\CoreBundle\Entity\ShopVoucherSeries" table="shop_voucher_series">




        <id name="id" type="uuid" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <id name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </id>
            <field name="name" column="name" type="string"  nullable="false"  />
            <field name="shopVoucherSeriesSponsorId" column="shop_voucher_series_sponsor_id" type="string"  nullable="false"  />
            <field name="value" column="value" type="string" length="10,2" nullable="false"  />
            <field name="valueType" column="value_type" type="string" length="&#039;absolut&#039;,&#039;prozent&#039;" nullable="false"  />
            <field name="shopVatId" column="shop_vat_id" type="string"  nullable="false"  />
            <field name="freeShipping" column="free_shipping" type="string"  nullable="false"  />
            <field name="active" column="active" type="string"  nullable="false"  />
            <field name="activeFrom" column="active_from" type="string"  nullable="false"  />
            <field name="activeTo" column="active_to" type="string"  nullable="false"  />
            <field name="restrictToValue" column="restrict_to_value" type="string" length="10,2" nullable="false"  />
            <field name="restrictToOtherSeries" column="restrict_to_other_series" type="string"  nullable="false"  />
            <field name="allowNoOtherVouchers" column="allow_no_other_vouchers" type="string"  nullable="false"  />
            <field name="restrictToOnePerUser" column="restrict_to_one_per_user" type="string"  nullable="false"  />
            <field name="restrictToFirstOrder" column="restrict_to_first_order" type="string"  nullable="false"  />
            <field name="dataExtranetUserMlt" column="data_extranet_user_mlt" type="string"  nullable="false"  />
            <field name="dataExtranetGroupMlt" column="data_extranet_group_mlt" type="string"  nullable="false"  />
            <field name="shopManufacturerMlt" column="shop_manufacturer_mlt" type="string"  nullable="false"  />
            <field name="shopArticleGroupMlt" column="shop_article_group_mlt" type="string"  nullable="false"  />
            <field name="shopCategoryMlt" column="shop_category_mlt" type="string"  nullable="false"  />
            <field name="shopArticleMlt" column="shop_article_mlt" type="string"  nullable="false"  />
            <field name="shopVoucher" column="shop_voucher" type="string"  nullable="false"  />
    </entity>

</doctrine-mapping>