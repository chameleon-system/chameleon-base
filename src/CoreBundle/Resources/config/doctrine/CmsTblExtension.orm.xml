<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTblExtension" table="cms_tbl_extension">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsTblConf" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblConf" inversed-by="cmsTblExtensionCollection" >
    <join-column name="cms_tbl_conf_id" referenced-column-name="id" />
</many-to-one>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="nameList" type="string" column="name_list" length="255" nullable="false"  />
            <field name="subtype" type="string" column="subtype" length="255" nullable="false"  />
            <field name="virtualItemClassName" type="string" column="virtual_item_class_name" length="255" nullable="false"  />
            <field name="virtualItemClassListName" type="string" column="virtual_item_class_list_name" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>