<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTblExtension" table="cms_tbl_extension">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsTblConfId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsTblConf" inversed-by="cmsTblExtension">
    <join-column name="cms_tbl_conf_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="nameList"  column="name_list"  nullable="false"  />
            <field name="subtype"  column="subtype"  nullable="false"  />
            <field name="type"  column="type"  nullable="false"  />
            <field name="position"  column="position"  nullable="false"  />
            <field name="virtualItemClassName"  column="virtual_item_class_name"  nullable="false"  />
            <field name="virtualItemClassListName"  column="virtual_item_class_list_name"  nullable="false"  />
    </entity>

</doctrine-mapping>