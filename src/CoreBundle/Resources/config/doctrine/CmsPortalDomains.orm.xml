<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsPortalDomains" table="cms_portal_domains">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsPortalId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsPortal" inversed-by="cmsPortalDomains">
    <join-column name="cms_portal_id" referenced-column-name="id" />
</many-to-one>
            <field name="name"  column="name"  nullable="false"  />
            <field name="sslname"  column="sslname"  nullable="false"  />
            <field name="cmsLanguageId" type="string" field="cms_language_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsLanguage" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsLanguage">
    <join-column name="cms_language_id" referenced-column-name="id" />
</many-to-one>
            <field name="isMasterDomain"  column="is_master_domain"  nullable="false"  />
            <field name="googleApiKey"  column="google_api_key"  nullable="false"  />
    </entity>

</doctrine-mapping>