<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsTblConf" table="cms_tbl_conf">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name" type="string" column="name" length="255" nullable="false"  />
            <field name="translation" type="string" column="translation" length="255" nullable="false"  />
            <one-to-many field="cmsTblFieldTabCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblFieldTab" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="cmsFieldConfMltCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsFieldConf" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="propertyListFieldsCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblDisplayListFields" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <one-to-many field="propertyOrderFieldsCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblDisplayOrderfields" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="nameColumn" type="string" column="name_column" length="255" nullable="false"  />
            <field name="nameColumnCallback" type="string" column="name_column_callback" length="255" nullable="false"  />
            <field name="displayColumn" type="string" column="display_column" length="255" nullable="false"  />
            <field name="displayColumnCallback" type="string" column="display_column_callback" length="255" nullable="false"  />
            <field name="listGroupField" type="string" column="list_group_field" length="255" nullable="false"  />
            <field name="listGroupFieldHeader" type="string" column="list_group_field_header" length="255" nullable="false"  />
            <field name="listGroupFieldColumn" type="string" column="list_group_field_column" length="255" nullable="false"  />
            <one-to-many field="cmsTblListClassCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblListClass" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="tableEditorClass" type="string" column="table_editor_class" length="255" nullable="false"  />
            <field name="tableEditorClassSubtype" type="string" column="table_editor_class_subtype" length="255" nullable="false"  />
            <one-to-many field="cmsTblConfRestrictionsCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblConfRestrictions" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="dbobjectExtendClass" type="string" column="dbobject_extend_class" length="255" nullable="false"  />
            <field name="dbobjectExtendSubtype" type="string" column="dbobject_extend_subtype" length="255" nullable="false"  />
            <one-to-many field="cmsTblExtensionCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblExtension" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="autoLimitResults" type="string" column="auto_limit_results" length="255" nullable="false"  />
            <one-to-many field="cmsTblConfIndexCollection" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsTblConfIndex" mapped-by="cmsTblConf" orphan-removal="true">
    <cascade-persist/>
</one-to-many>

            <field name="iconFontCssClass" type="string" column="icon_font_css_class" length="255" nullable="false"  />
    </entity>

</doctrine-mapping>