<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsPortal" table="cms_portal">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <field name="name"  column="name"  nullable="false"  />
            <field name="title"  column="title"  nullable="false"  />
            <field name="identifier"  column="identifier"  nullable="false"  />
            <field name="externalIdentifier"  column="external_identifier"  nullable="false"  />
            <field name="cmsLanguageId" type="string" field="cms_language_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsLanguage" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsLanguage">
    <join-column name="cms_language_id" referenced-column-name="id" />
</many-to-one>
            <field name="useMultilanguage"  column="use_multilanguage"  nullable="false"  />
            <field name="showNotTanslated"  column="show_not_tanslated"  nullable="false"  />
            <field name="mainNodeTree"  column="main_node_tree"  nullable="false"  />
            <one-to-many field="propertyNavigationsCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsPortalNavigation" mapped-by="cmsPortalId" />

            <field name="homeNodeId"  column="home_node_id"  nullable="false"  />
            <field name="pageNotFoundNode"  column="page_not_found_node"  nullable="false"  />
            <one-to-many field="cmsPortalDivisionsCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsDivision" mapped-by="cmsPortalId" />

            <field name="sortOrder"  column="sort_order"  nullable="false"  />
            <one-to-many field="cmsPortalDomainsCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsPortalDomains" mapped-by="cmsPortalId" />

            <field name="faviconUrl"  column="favicon_url"  nullable="false"  />
            <field name="imagesId" type="string" field="images_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="images" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="images_id" referenced-column-name="id" />
</many-to-one>
            <field name="watermarkLogoId" type="string" field="watermark_logo_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="watermarkLogo" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="watermark_logo_id" referenced-column-name="id" />
</many-to-one>
            <field name="backgroundImageId" type="string" field="background_image_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="backgroundImage" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsMedia">
    <join-column name="background_image_id" referenced-column-name="id" />
</many-to-one>
            <one-to-many field="cmsMessageManagerMessageCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsMessageManagerMessage" mapped-by="cmsPortalId" />

            <one-to-many field="cmsPortalSystemPageCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsPortalSystemPage" mapped-by="cmsPortalId" />

            <many-to-many field="cmsLanguageMlt" target-entity="ChameleonSystem\CoreBundle\Entity\CmsLanguage">
    <join-table name="cms_portal_cms_language_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
</many-to-many>
            <field name="useGooglesitemap"  column="use_googlesitemap"  nullable="false"  />
            <field name="metaDescription"  column="meta_description"  nullable="false"  />
            <field name="metaKeywords"  column="meta_keywords"  nullable="false"  />
            <field name="metaAuthor"  column="meta_author"  nullable="false"  />
            <field name="metaPublisher"  column="meta_publisher"  nullable="false"  />
            <field name="cmsLocalsId" type="string" field="cms_locals_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="cmsLocals" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsLocals">
    <join-column name="cms_locals_id" referenced-column-name="id" />
</many-to-one>
            <field name="customMetadata"  column="custom_metadata"  nullable="false"  />
            <field name="pkgCmsThemeId" type="string" field="pkg_cms_theme_id" length="36">
    <options>
        <option name="default"></option>
    </options>
</field>
<many-to-one field="pkgCmsTheme" target-entity="\ChameleonSystem\CoreBundle\Entity\PkgCmsTheme">
    <join-column name="pkg_cms_theme_id" referenced-column-name="id" />
</many-to-one>
            <field name="actionPluginList"  column="action_plugin_list"  nullable="false"  />
            <field name="googleAnalyticNumber"  column="google_analytic_number"  nullable="false"  />
            <field name="etrackerId"  column="etracker_id"  nullable="false"  />
            <field name="ivwId"  column="ivw_id"  nullable="false"  />
            <field name="indexSearch"  column="index_search"  nullable="false"  />
            <field name="useSlashInSeoUrls"  column="use_slash_in_seo_urls"  nullable="false"  />
            <field name="deactivePortal"  column="deactive_portal"  nullable="false"  />
            <field name="wysiwygCssUrl"  column="wysiwyg_css_url"  nullable="false"  />
            <one-to-many field="cmsUrlAliasCollection" target-entity="ChameleonSystem\CoreBundle\Entity\CmsUrlAlias" mapped-by="cmsPortalId" />

            <field name="robots"  column="robots"  nullable="false"  />
    </entity>

</doctrine-mapping>