<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\ShopArticleDocument" table="shop_article_document">




        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="shopArticle" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopArticle" inversed-by="shopArticleDocumentCollection" >
    <join-column name="shop_article_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="shopArticleDocumentType" target-entity="\ChameleonSystem\CoreBundle\Entity\ShopArticleDocumentType">
    <join-column name="shop_article_document_type_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="cmsDocument" target-entity="\ChameleonSystem\CoreBundle\Entity\CmsDocument">
    <join-column name="cms_document_id" referenced-column-name="id" />
</many-to-one>
    </entity>

</doctrine-mapping>