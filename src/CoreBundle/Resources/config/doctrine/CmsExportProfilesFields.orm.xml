<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\CmsExportProfilesFields" table="cms_export_profiles_fields">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="cmsExportProfilesId" target-entity="ChameleonSystem\CoreBundle\Entity\CmsExportProfiles" inversed-by="cmsExportProfilesFields">
    <join-column name="cms_export_profiles_id" referenced-column-name="id" />
</many-to-one>
            <field name="fieldname"  column="fieldname"  nullable="false"  />
            <field name="sortOrder"  column="sort_order"  nullable="false"  />
            <field name="htmlTemplate"  column="html_template"  nullable="false"  />
    </entity>

</doctrine-mapping>