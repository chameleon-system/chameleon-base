<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessageTrigger" table="pkg_shop_payment_ipn_message_trigger">




        <id name="id" type="string" length="36" column="id" unique="true">
            <generator strategy="CUSTOM"/>
            <custom-id-generator class="doctrine.uuid_generator"/>
        </id>
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
        </field>
            <many-to-one field="pkgShopPaymentIpnTriggerId" target-entity="ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnTrigger" inversed-by="pkgShopPaymentIpnMessageTrigger">
    <join-column name="pkg_shop_payment_ipn_trigger_id" referenced-column-name="id" />
</many-to-one>
            <many-to-one field="pkgShopPaymentIpnMessageId" target-entity="ChameleonSystem\CoreBundle\Entity\PkgShopPaymentIpnMessage" inversed-by="pkgShopPaymentIpnMessageTrigger">
    <join-column name="pkg_shop_payment_ipn_message_id" referenced-column-name="id" />
</many-to-one>
            <field name="datecreated"  column="datecreated"  nullable="false">
    <options>
        <option name="default">CURRENT_TIMESTAMP</option>
    </options>
</field>
            <field name="done"  column="done"  nullable="false"  />
            <field name="doneDate"  column="done_date"  nullable="false"  />
            <field name="success"  column="success"  nullable="false"  />
            <field name="attemptCount"  column="attempt_count"  nullable="false"  />
            <field name="nextAttempt"  column="next_attempt"  nullable="false"  />
            <field name="log"  column="log"  nullable="false"  />
    </entity>

</doctrine-mapping>